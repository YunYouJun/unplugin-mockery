{
  "name": "unplugin-mockery",
  "type": "module",
  "version": "0.3.12",
  "packageManager": "pnpm@9.12.3",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/YunYouJun/unplugin-mockery#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunYouJun/unplugin-mockery.git"
  },
  "bugs": {
    "url": "https://github.com/YunYouJun/unplugin-mockery/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./widget": {
      "types": "./widget/dist/index.d.ts",
      "import": "./widget/dist/index.mjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "client/dist",
    "dist",
    "dist-client",
    "widget/dist"
  ],
  "scripts": {
    "build": "npm run build:unplugin && npm run build:client && npm run build:widget",
    "build:unplugin": "tsup",
    "build:client": "pnpm -C client run build",
    "build:widget": "pnpm -C widget run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "dev": "tsup --watch src",
    "dev:client": "pnpm -C client run dev",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "play:webpack": "pnpm -C playground/webpack run serve",
    "play:vite": "pnpm -C playground/vite run dev",
    "play:vue-cli": "pnpm -C playground/vue-cli run serve",
    "play:nuxt": "pnpm -C playground/nuxt run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp --execute 'npm run changelog' --all && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "ajv": "^8.17.1",
    "chokidar": "^4.0.1",
    "consola": "^3.2.3",
    "express": "^4.21.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jiti": "^2.4.0",
    "launch-editor": "^2.9.1",
    "open": "^10.1.0",
    "path-to-regexp": "^8.2.0",
    "picocolors": "^1.1.1",
    "serve-static": "^1.16.2",
    "unplugin": "^1.15.0"
  },
  "devDependencies": {
    "@advjs/gui": "0.0.7-beta.5",
    "@antfu/eslint-config": "^3.8.0",
    "@nuxt/kit": "^3.14.159",
    "@nuxt/schema": "^3.14.159",
    "@trpc/client": "11.0.0-rc.477",
    "@trpc/server": "11.0.0-rc.477",
    "@types/escape-html": "^1.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.9.0",
    "@unocss/eslint-config": "^0.64.0",
    "bumpp": "^9.8.1",
    "chalk": "^5.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.2",
    "esno": "^4.8.0",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.4",
    "sass": "^1.80.6",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "webpack": "^5.96.1",
    "zod": "^3.23.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix --no-warn-ignored"
  }
}
