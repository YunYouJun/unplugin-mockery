    "pinia": "^2.1.7",
    "vue": "^3.4.34",
    "vue-demi": "^0.14.10",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.36",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@shikijs/markdown-it": "^1.11.2",
